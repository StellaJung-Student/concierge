{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": true,
        "outDir": "../dist",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "lib": [
            "es2015",
            "dom"
        ]
    },
    "include": [
        "../src/**/*.ts",
        "../typings/**/*.ts"
    ],
    "exclude": [
        "../src/__deprecated/**/*ts"
    ]
}