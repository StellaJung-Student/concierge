    "compression": "^1.6.2",
    "dockerode": "^2.4.3",
    "dockerode-ts": "^0.2.11",
    "express": "^4.14.1",
    "http-proxy": "^1.13.3",
    "knex": "^0.11.7",
    "knockout": "^3.4.1",
    "minimist": "^1.2.0",
    "node-dockerfile": "^0.5.1",
    "observers": "^0.5.1",
    "promise-polyfill": "^6.0.2",
    "rimraf": "^2.5.3",
    "socket.io": "^1.5.0",
    "sqlite3": "^3.1.4",
    "ssh2": "^0.4.9",
    "tar-fs": "^1.5.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/history": "^2.0.39",
    "@types/knex": "^0.0.53",
    "@types/knockout": "^3.4.39",
    "@types/minimist": "^1.2.0",
    "@types/node": "^6.0.38",
    "brfs": "^1.4.3",
    "browserify": "^14.1.0",
    "browserify-fs": "^1.0.0",
    "chai": "^3.4.1",
    "mocha": "^2.2.5",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "test": "mocha",
    "debug": "node --inspect start.js",
    "start": "node .",
    "build:front": "tsc -p tsconfig.front.json && yarn bundle",
    "build:back": "tsc -p tsconfig.node.json",
    "build": "tsc -p tsconfig.node.json && tsc -p tsconfig.front.json",
    "postbuild": "browserify -e front/index.js -o front/bundle.js -r fs:browserify-fs -t brfs",
    "bundle": "browserify -e front/index.js -o front/bundle.js -r fs:browserify-fs -t brfs",
    "lint": "tslint --project . --exclude \"repositories/**\" --rules tslint.json",
    "create-cert": "openssl req -newkey rsa:2048 -nodes -keyout certificates/registry.key -x509 -days 999 -out certificates/registry.crt -subj \"/CN=localhost\"",
    "create-registry": "npm run create-cert && docker-compose -f registry.compose.yml up -d"
  },
