{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "noImplicitReturns": true,
        "lib": [
            "es2015",
            "dom"
        ]
    },
    "include": [
        "src/*.ts",
        "src/proxy/**/*.ts",
        "src/api/**/*.ts",
        "front/**/*.ts",
        "src/data/*.ts",
        "typings/**/*.ts",
        "src/server/**/*.ts",
        "src/configurations/**/*.ts"
    ]
}