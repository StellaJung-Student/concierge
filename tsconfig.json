{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": ["es2015", "dom"],
    "newLine": "LF"
  },
  "include": ["src/**/*.ts", "front/**/*.ts", "typings/**/*.ts"],
  "exclude": ["src/__deprecated/**/*.ts"]
}
