// Place your settings in this file to overwrite default and user settings.
{
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/archive/*.db": true,
		"**/archive/*.tar": true,
		"**/repositories": true,
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*?.js": {
			"when": "$(basename).tsx"
		},
		"node_modules": true
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"editor.tabSize": 2,
	"files.eol": "\n"
}