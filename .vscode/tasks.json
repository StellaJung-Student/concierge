{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "yarn",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "problemMatcher": "$tsc",
  "tasks": [
    {
      "isBuildCommand": true,
      "taskName": "build:back",
      "args": [
        "build:back"
      ],
      "problemMatcher": "$tsc"
    },
    {
      "isBuildCommand": true,
      "taskName": "build:front",
      "args": [
        "build:front"
      ],
      "problemMatcher": "$tsc"
    }
  ]
}